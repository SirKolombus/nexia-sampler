{
  "root": true,
  "env": {
    "browser": true,
    "es2021": true
  },
  "globals": {
    "Excel": "readonly",
    "Office": "readonly",
    "localStorage": "readonly",
    "setTimeout": "readonly"
  },
  "plugins": [
    "office-addins"
  ],
  "extends": [
    "plugin:office-addins/recommended"
  ],
  "rules": {
    "no-empty": ["error", {"allowEmptyCatch": true}]
  }
}
